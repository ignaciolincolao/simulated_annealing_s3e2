# Define the groups in each area
sep <- c(100,0)
nosep <- c(0,100)
# Calculate the total for each group
sepc <- sum(sep)
nosepc <- sum(nosep)
# Calculate the proportion of each group in each area
prop_sep <- sep / sepc
prop_nosep <- nosep / nosepc
# Compute the absolute differences in proportions
abs_diff <- abs(prop_sep - prop_nosep)
# Compute D index
D <- 0.5 * sum(abs_diff)
# Print the result
D
# Define the counts of students in each area for each class
sep <- c(100,0)
nosep <- c(0,100)
# Compute the total number of students in each area
t <- sep + nosep
# Compute the total number of students across all areas
tc <- sum(t)
# Calculate the total for each class
sepc <- sum(sep)
nosepc <- sum(nosep)
# Calculate the proportion of each class in each area
prop_sep <- sep / sepc
prop_nosep <- nosep / nosepc
# Compute the absolute differences in proportions
abs_diff <- abs(prop_sep - prop_nosep)
# Calculate the relative weight of each unit (area)
weights <- t / tc
# Compute D index using the weighted sum of absolute differences
D <- 0.5 * sum(weights * abs_diff)
# Print the result
D
